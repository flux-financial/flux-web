{
  "hosting": {
    "public": "dist",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|webp|png|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000, s-maxage=7200"
          }
        ]
      }
    ]
  }
}
